<!-- GSAP + ScrollTrigger Animations -->
<script>
gsap.registerPlugin(ScrollTrigger);

// Animate sections and cards
gsap.utils.toArray("section, .card, .experience-card, .edu-card, .project-card, .video-card").forEach(elem => {
  gsap.from(elem, {
    scrollTrigger: {
      trigger: elem,
      start: "top 85%",
      toggleActions: "play none none none"
    },
    opacity: 0,
    y: 50,
    duration: 1,
    ease: "power3.out",
    stagger: 0.2
  });
});

// Smooth scrolling
$(".nav-link a").on("click", function(e){
  e.preventDefault();
  let target = $(this).attr("href");
  $("html, body").animate({scrollTop: $(target).offset().top - 70}, 800);
});

// ScrollSpy navbar
$(window).on("scroll", function(){
  let scrollPos = $(document).scrollTop();
  $(".nav-link a").each(function(){
    let currLink = $(this);
    let refElement = $(currLink.attr("href"));
    if(refElement.position().top - 80 <= scrollPos && refElement.position().top + refElement.height() > scrollPos){
      $(".nav-link").removeClass("active");
      currLink.parent().addClass("active");
    }
  });
});
</script>


    <style>
      :root {
        --royal-blue: #0f172a;
        --gold-premium: #c5a059;
        --gold-soft: #e5d5b7;
        --slate-50: #f8fafc;
        --slate-200: #e2e8f0;
        --slate-600: #475569;
      }

      body {
        background-color: #d1d5db;
        font-family: 'Inter', sans-serif;
        color: var(--royal-blue);
        padding: 40px 0;
      }

      .resume-canvas {
        background: white;
        max-width: 1100px;
        margin: 0 auto;
        box-shadow: 0 40px 80px rgba(0, 0, 0, 0.15);
        border-radius: 8px;
        overflow: hidden;
      }

      /* Premium Header */
      .header-hero {
        background: linear-gradient(135deg, var(--royal-blue) 0%, #1e293b 100%);
        color: white;
        padding: 80px 60px;
        position: relative;
      }

      .header-hero::after {
        content: "";
        position: absolute;
        top: 0; right: 0; bottom: 0; left: 0;
        background: url('https://www.transparenttextures.com/patterns/cubes.png');
        opacity: 0.05;
      }

      .name-text {
        font-family: 'DM Serif Display', serif;
        font-size: 4rem;
        line-height: 1;
        letter-spacing: -1px;
      }

      .role-badge {
        display: inline-block;
        color: var(--gold-premium);
        font-weight: 600;
        letter-spacing: 4px;
        text-transform: uppercase;
        font-size: 0.9rem;
        margin-bottom: 25px;
      }

      .contact-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: 15px;
        margin-top: 30px;
        border-top: 1px solid rgba(255,255,255,0.1);
        padding-top: 30px;
      }

      .contact-item {
        color: rgba(255,255,255,0.8);
        text-decoration: none;
        font-size: 0.85rem;
        display: flex;
        align-items: center;
        gap: 10px;
        transition: 0.3s;
      }

      .contact-item:hover { color: var(--gold-premium); }

      /* Sidebar Aesthetics */
      .sidebar-column {
        background-color: var(--slate-50);
        padding: 50px 40px;
        border-right: 1px solid var(--slate-200);
      }

      .section-heading {
        font-family: 'DM Serif Display', serif;
        font-size: 1.4rem;
        color: var(--royal-blue);
        margin-bottom: 25px;
        display: flex;
        align-items: center;
        gap: 10px;
      }

      .section-heading::after {
        content: "";
        flex-grow: 1;
        height: 1px;
        background: var(--slate-200);
      }

      /* Skills Bars */
      .skill-box { margin-bottom: 20px; }
      .skill-info {
        display: flex;
        justify-content: space-between;
        font-size: 0.8rem;
        font-weight: 700;
        margin-bottom: 6px;
        color: var(--slate-600);
      }
      .progress {
        height: 6px;
        background: var(--slate-200);
        overflow: visible;
        border-radius: 10px;
      }
      .progress-bar {
        background: linear-gradient(90deg, var(--gold-premium), #9a7d45);
        border-radius: 10px;
        position: relative;
      }
      .progress-bar::after {
        content: "";
        position: absolute;
        right: 0; top: -4px;
        width: 14px; height: 14px;
        background: white;
        border: 3px solid var(--gold-premium);
        border-radius: 50%;
      }

      /* Content Area */
      .main-column { padding: 60px; }

      .summary-quote {
        font-size: 1.1rem;
        line-height: 1.8;
        color: var(--slate-600);
        font-weight: 500;
        margin-bottom: 50px;
        padding: 30px;
        background: var(--slate-50);
        border-radius: 0 20px 20px 20px;
        border-left: 5px solid var(--gold-premium);
      }

      .timeline-item {
        position: relative;
        padding-left: 30px;
        padding-bottom: 40px;
        border-left: 2px solid var(--slate-200);
      }

      .timeline-item::before {
        content: "";
        position: absolute;
        left: -9px; top: 0;
        width: 16px; height: 16px;
        background: white;
        border: 4px solid var(--gold-premium);
        border-radius: 50%;
      }

      .exp-title { font-weight: 800; font-size: 1.25rem; color: var(--royal-blue); }
      .exp-company { color: var(--gold-premium); font-weight: 700; font-size: 0.85rem; letter-spacing: 1px; text-transform: uppercase; }
      .exp-date { font-size: 0.8rem; font-weight: 600; color: var(--slate-600); margin-bottom: 15px; }

      /* Projects & Education */
      .premium-card {
        border: 1px solid var(--slate-200);
        padding: 25px;
        border-radius: 4px;
        margin-bottom: 25px;
        transition: 0.4s;
      }
      .premium-card:hover { border-color: var(--gold-premium); background: #fffcf8; }

      .btn-gold {
        background: var(--gold-premium);
        color: white;
        border: none;
        padding: 8px 20px;
        font-size: 0.75rem;
        font-weight: 700;
        text-transform: uppercase;
        border-radius: 2px;
        text-decoration: none;
        transition: 0.3s;
      }
      .btn-gold:hover { background: var(--royal-blue); color: white; }

      @media print {
        body { padding: 0; background: white; }
        .resume-canvas { box-shadow: none; max-width: 100%; border-radius: 0; }
        .no-print { display: none; }
        .header-hero { background: var(--royal-blue) !important; -webkit-print-color-adjust: exact; }
      }
    </style>

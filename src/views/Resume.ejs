    <style>
        :root {
            --primary-dark: #0f172a; /* Deep Midnight */
            --accent-gold: #c5a059;  /* Refined Gold */
            --bg-body: #f1f5f9;
            --text-main: #1e293b;
            --text-muted: #64748b;
            --sidebar-bg: #f8fafc;
        }

        body { 
            background-color: var(--bg-body); 
            font-family: 'Inter', sans-serif; 
            color: var(--text-main);
            padding: 40px 0;
        }

        .resume-card {
            background: white;
            max-width: 1100px;
            margin: 0 auto;
            box-shadow: 0 40px 100px rgba(15, 23, 42, 0.15);
            border-radius: 8px;
            overflow: hidden;
            border-top: 8px solid var(--accent-gold);
        }

        /* Header Section */
        .header-section {
            background: var(--primary-dark);
            color: white;
            padding: 60px 50px;
            position: relative;
        }

        .name-title {
            font-family: 'DM Serif Display', serif;
            font-size: 4rem;
            line-height: 1;
            margin-bottom: 5px;
            letter-spacing: -1px;
        }

        .sub-title {
            color: var(--accent-gold);
            letter-spacing: 4px;
            text-transform: uppercase;
            font-weight: 600;
            font-size: 0.85rem;
            margin-bottom: 30px;
            display: block;
        }

        /* Contact Grid - Single Row Fix */
        .contact-grid-single-row {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            margin-top: 25px;
            padding-top: 25px;
            border-top: 1px solid rgba(255, 255, 255, 0.1);
        }

        .contact-item {
            color: rgba(255, 255, 255, 0.8);
            text-decoration: none;
            font-size: 0.85rem;
            display: flex;
            align-items: center;
            gap: 8px;
            transition: 0.3s ease;
        }

        .contact-item i { color: var(--accent-gold); }
        .contact-item:hover { color: white; transform: translateY(-1px); }

        /* Sidebar */
        .sidebar { background: var(--sidebar-bg); border-right: 1px solid #e2e8f0; padding: 50px 40px; }
        
        .section-label {
            font-weight: 800;
            text-transform: uppercase;
            font-size: 0.75rem;
            letter-spacing: 2px;
            color: var(--primary-dark);
            margin-bottom: 25px;
            display: block;
            border-left: 4px solid var(--accent-gold);
            padding-left: 15px;
        }

        .skill-name { font-size: 0.8rem; font-weight: 700; color: var(--text-main); margin-bottom: 6px; display: block; }
        .progress { height: 5px; background-color: #e2e8f0; border-radius: 10px; margin-bottom: 18px; }
        .progress-bar { background: var(--accent-gold); border-radius: 10px; }

        /* Content Area */
        .main-body { padding: 50px 60px; }
        
        .summary-text {
            font-size: 1.05rem;
            line-height: 1.8;
            color: var(--text-muted);
            margin-bottom: 45px;
            padding-left: 20px;
            border-left: 1px solid #e2e8f0;
        }

        .exp-title { font-weight: 800; font-size: 1.3rem; color: var(--primary-dark); }
        .exp-meta { color: var(--accent-gold); font-weight: 700; font-size: 0.9rem; margin-bottom: 12px; }
        
        .responsibility-list li {
            font-size: 0.95rem;
            color: var(--text-muted);
            margin-bottom: 10px;
            line-height: 1.6;
        }

        .project-item {
            border: 1px solid #f1f5f9;
            padding: 25px;
            border-radius: 12px;
            margin-bottom: 25px;
            transition: all 0.3s ease;
            background: #fff;
        }
        
        .project-item:hover { 
            border-color: var(--accent-gold); 
            box-shadow: 0 10px 30px rgba(0,0,0,0.05);
            transform: translateY(-3px);
        }

        .btn-premium {
            background: var(--primary-dark);
            color: white;
            padding: 8px 18px;
            font-size: 0.75rem;
            font-weight: 700;
            border-radius: 6px;
            text-decoration: none;
            transition: 0.3s;
            border: 1px solid var(--primary-dark);
        }

        .btn-premium:hover { background: var(--accent-gold); border-color: var(--accent-gold); color: white; }

        .edu-item b { color: var(--primary-dark); }

        @media print {

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.0/font/bootstrap-icons.css">

  <style>
    :root {
      --brand-dark: #0a0a0a;
      --brand-accent: #c5a059; /* Metallic Bronze */
      --brand-muted: #6b7280;
      --brand-bg: #f9fafb;
    }

    body { 
      background-color: #d1d5db; 
      font-family: 'Inter', sans-serif; 
      color: var(--brand-dark); 
      padding: 60px 0;
    }

    .master-container {
      background: white;
      max-width: 1100px;
      margin: 0 auto;
      box-shadow: 0 50px 100px -20px rgba(0,0,0,0.2);
      border-radius: 4px;
    }

    /* Side Branding */
    .brand-sidebar {
      background: var(--brand-dark);
      color: white;
      padding: 60px 40px;
      height: 100%;
    }

    .initial-logo {
      width: 50px;
      height: 50px;
      border: 1px solid var(--brand-accent);
      display: flex;
      align-items: center;
      justify-content: center;
      font-family: 'DM Serif Display', serif;
      font-size: 1.5rem;
      color: var(--brand-accent);
      margin-bottom: 40px;
    }

    .main-content { padding: 80px 60px; }

    /* Typography */
    .name-heading {
      font-family: 'DM Serif Display', serif;
      font-size: 4rem;
      line-height: 0.9;
      margin-bottom: 10px;
    }

    .job-subtitle {
      font-weight: 300;
      letter-spacing: 4px;
      text-transform: uppercase;
      color: var(--brand-accent);
      font-size: 0.9rem;
      margin-bottom: 60px;
    }

    .section-title {
      font-family: 'DM Serif Display', serif;
      font-size: 1.8rem;
      border-bottom: 1px solid #eee;
      padding-bottom: 15px;
      margin-bottom: 30px;
    }

    /* Skill Grid */
    .skill-category { font-size: 0.75rem; font-weight: 700; color: var(--brand-accent); letter-spacing: 1px; margin-bottom: 10px; }
    .skill-item { font-weight: 500; font-size: 0.95rem; margin-bottom: 8px; }

    /* Experience Card */
    .exp-row { margin-bottom: 40px; position: relative; }
    .exp-year { font-weight: 700; font-size: 0.85rem; color: var(--brand-accent); }
    .exp-company { font-weight: 700; font-size: 1.1rem; margin-top: 5px; }
    .exp-role { font-style: italic; color: var(--brand-muted); font-size: 0.9rem; margin-bottom: 10px; }
    
    .stat-badge {
      background: #fdfaf3;
      border-left: 3px solid var(--brand-accent);
      padding: 10px 15px;
      font-size: 0.85rem;
      font-weight: 600;
      margin-top: 15px;
    }

    @media print {
      body { padding: 0; background: white; }
      .master-container { box-shadow: none; max-width: 100%; border-radius: 0; }
      .no-print { display: none; }
      .brand-sidebar { background: #0a0a0a !important; -webkit-print-color-adjust: exact; }
    }
  </style>
</head>

        .contact-link:hover { color: var(--accent-gold); }

        /* Sidebar Progress Bars */
        .sidebar { background: var(--sidebar-bg); border-right: 1px solid #f1f5f9; padding: 40px; }
        
        .section-label {
            font-weight: 700;
            text-transform: uppercase;
            font-size: 0.75rem;
            letter-spacing: 1.5px;
            color: var(--accent-gold);
            margin-bottom: 25px;

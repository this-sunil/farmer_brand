        .progress-bar { background-color: var(--accent); border-radius: 10px; position: relative; }
        .progress-bar::after {
            content: "";
            position: absolute;
            right: 0;
            top: -4px;
            width: 14px;
            height: 14px;
            background: white;
            border: 3px solid var(--accent);
            border-radius: 50%;
        }


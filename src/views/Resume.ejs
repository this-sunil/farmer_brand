    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.0/font/bootstrap-icons.css" />
    <style>
      :root {
        --primary: #0f172a;
        --accent: #0ea5e9;
        --text-main: #1e293b;
        --text-muted: #64748b;
        --bg-light: #f8fafc;
      }
      body {
        background-color: #e2e8f0;
        font-family: 'Inter', sans-serif;
        color: var(--text-main);
        padding: 40px 0;
      }
      .resume-container {
        background: white;
        max-width: 1000px;
        margin: 0 auto;
        border-radius: 12px;
        overflow: hidden;
        box-shadow: 0 20px 50px rgba(0,0,0,0.1);
      }
      .header {
        background: var(--primary);
        color: white;
        padding: 50px;
        border-bottom: 5px solid var(--accent);
      }
      .name { font-weight: 800; font-size: 3rem; letter-spacing: -1px; }
      .role { color: var(--accent); font-weight: 600; text-transform: uppercase; letter-spacing: 2px; font-size: 1rem; }
      
      .sidebar { background: var(--bg-light); padding: 40px; border-right: 1px solid #e2e8f0; }
      .main-content { padding: 40px 50px; }
      
      .section-title {
        font-weight: 800;
        text-transform: uppercase;
        font-size: 0.9rem;
        color: var(--primary);
        margin-bottom: 20px;
        display: flex;
        align-items: center;
        gap: 10px;
      }
      .section-title::after { content: ""; flex: 1; height: 1px; background: #e2e8f0; }

      .exp-item { margin-bottom: 30px; }
      .exp-header { display: flex; justify-content: space-between; align-items: baseline; }
      .company { font-weight: 700; color: var(--primary); font-size: 1.1rem; }
      .duration { font-size: 0.85rem; color: var(--accent); font-weight: 600; }
      .job-title { font-weight: 600; font-size: 0.95rem; margin-bottom: 8px; color: #475569; }
      
      .skill-tag {
        display: inline-block;
        background: #fff;
        border: 1px solid #e2e8f0;
        padding: 4px 12px;
        border-radius: 20px;
        font-size: 0.8rem;
        margin: 0 4px 8px 0;
        font-weight: 500;
      }
      
      .bullet-list { list-style: none; padding-left: 0; }
      .bullet-list li { 
        position: relative; 
        padding-left: 20px; 
        margin-bottom: 6px; 
        font-size: 0.9rem; 
        color: var(--text-muted);
        line-height: 1.5;
      }
      .bullet-list li::before { 
        content: "•"; 
        color: var(--accent); 
        position: absolute; 
        left: 0; 
        font-weight: bold; 
      }

      @media print {
        body { padding: 0; background: white; }
        .resume-container { box-shadow: none; border-radius: 0; max-width: 100%; }
        .header { background: #0f172a !important; -webkit-print-color-adjust: exact; }
      }
    </style>

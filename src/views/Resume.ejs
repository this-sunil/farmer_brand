            .resume-card { box-shadow: none; max-width: 100%; border-radius: 0; border-top: none; }
            .no-print { display: none; }
            .header-section { background: #0f172a !important; -webkit-print-color-adjust: exact; }
            .progress-bar { background: #c5a059 !important; -webkit-print-color-adjust: exact; }
        }
    </style>
</head>

.contact-grid-single-row {
    display: flex;
    flex-wrap: wrap; /* Chhoti screen par automatic adjust hoga */
    gap: 20px;
    margin-top: 25px;
    padding-top: 20px;
    border-top: 1px solid rgba(255, 255, 255, 0.15);
    justify-content: flex-start; /* Baaye se shuru hoga */
}

.contact-item {
    color: rgba(255, 255, 255, 0.85);
    text-decoration: none;
    font-size: 0.8rem; /* Font thoda chhota rakha hai taaki 1 line mein aa sake */
    font-weight: 500;
    display: flex;
    align-items: center;
    gap: 8px;
    transition: 0.3s ease;
    white-space: nowrap; /* Text ko break hone se rokega */
}

.contact-item i {
    color: var(--gold-premium);
    font-size: 0.9rem;
}

.contact-item:hover {
    color: var(--gold-premium);
    transform: translateY(-2px);
}
        body { 

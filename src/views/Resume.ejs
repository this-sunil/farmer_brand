  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.0/font/bootstrap-icons.css">
  <style>
    :root {
      --primary-color: #1a2a6c;
      --accent-color: #00d2ff;
      --text-main: #2d3436;
      --text-muted: #636e72;
    }
    body { 
      background-color: #f0f2f5; 
      color: var(--text-main); 
      font-family: 'Inter', -apple-system, sans-serif;
      -webkit-print-color-adjust: exact;
    }
    .resume-container {
      max-width: 950px;
      margin: 40px auto;
      background: white;
      box-shadow: 0 20px 50px rgba(0,0,0,0.1);
      border-radius: 8px;
      overflow: hidden;
      border-top: 6px solid var(--primary-color);
    }
    .left-col { background: #fcfcfd; border-right: 1px solid #edf2f7; padding: 40px 30px; }
    .right-col { padding: 40px; }
    
    h1 { font-weight: 800; letter-spacing: -1px; color: var(--primary-color); }
    .section-header {
      font-weight: 700;
      text-transform: uppercase;
      font-size: 0.85rem;
      letter-spacing: 1.5px;
      color: var(--primary-color);
      border-bottom: 2px solid #edf2f7;
      margin-bottom: 15px;
      padding-bottom: 5px;
    }
    .skill-tag {
      display: inline-block;
      background: #f1f3f5;
      color: #495057;
      padding: 4px 10px;
      border-radius: 5px;
      font-size: 0.8rem;
      font-weight: 600;
      margin-bottom: 6px;
      margin-right: 4px;
      transition: all 0.3s ease;
    }
    .skill-tag:hover { background: var(--primary-color); color: white; }
    .exp-item { position: relative; padding-bottom: 25px; }
    .exp-title { font-weight: 700; color: #1e293b; font-size: 1.05rem; }
    .exp-meta { font-size: 0.85rem; color: var(--accent-color); font-weight: 600; margin-bottom: 8px; }
    
    .contact-link { text-decoration: none; color: var(--text-muted); font-size: 0.9rem; transition: 0.2s; }
    .contact-link:hover { color: var(--primary-color); }
    
    @media print {
      body { background: white; margin: 0; }
      .resume-container { box-shadow: none; margin: 0; max-width: 100%; border-radius: 0; }
      .no-print { display: none; }
    }
  </style>
</head>
